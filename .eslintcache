 {\r\n\r\n        case \"SET-PROJECTS\":\r\n            return {\r\n                ...state,\r\n                projects: action.data,\r\n            };\r\n\r\n        case \"UPDATE-PROJECTS\":\r\n            debugger\r\n            return {\r\n                ...state,\r\n                devices: updateObjectArray(state.projects, action.data.id, \"id\", action.data)\r\n            };\r\n\r\n        case \"SET-ICON\":\r\n            return {\r\n                ...state,\r\n                iconSelected: action.data,\r\n            };\r\n\r\n        case \"SET-PROJECT\":\r\n            return {\r\n                ...state,\r\n                project: action.data,\r\n            };\r\n\r\n        case \"LOGOUT\":\r\n            return initialState;\r\n\r\n        default:\r\n            return state;\r\n    }\r\n};\r\n// Action\r\nconst setProjects = (projects) => {\r\n    debugger\r\n    return {\r\n        type: \"SET-PROJECTS\",\r\n        data: projects,\r\n    };\r\n};\r\n\r\nconst updateProjects = (projects) => {\r\n    return {\r\n        type: \"UPDATE-PROJECTS\",\r\n        data: projects,\r\n    };\r\n};\r\n\r\nconst setIcon = (iconSelected) => {\r\n    return {\r\n        type: \"SET-ICON\",\r\n        data: iconSelected,\r\n    };\r\n};\r\n\r\nconst setProject = (project) => {\r\n    return {\r\n        type: \"SET-PROJECT\",\r\n        data: project\r\n    }\r\n}\r\n\r\n// ActionCreator\r\nexport const setIconActionCreator = (iconSelected) => {\r\n    return async (dispatch) => {\r\n        dispatch(setIcon(iconSelected));\r\n    };\r\n}\r\n\r\n// Redux-Thunk\r\n\r\n// Dashboard CRUD\r\n\r\nexport const getProjectPageThunkCreator = (username, token, page) => {\r\n    return async (dispatch) => {\r\n        const response = await projectAPI.getProjectPage(username, token, page);\r\n        dispatch(setProjects(response));\r\n    };\r\n};\r\n\r\nexport const createProjectThunkCreator = (projectForm, token, username, page, projectLength, setError) => {\r\n  